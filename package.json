{
  "name": "kasu",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "dev:https": "next dev --turbo --experimental-https",
    "dev:turbo": "next dev --turbo",
    "dev:inspect": "NODE_OPTIONS='--enable-source-maps --inspect' npm run dev:turbo",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "prepare": "husky install",
    "format": "prettier -w .",
    "format:check": "prettier -c . '!build/**/*'",
    "generate-types": "typechain --target=ethers-v5 --always-generate-overloads --out-dir=src/contracts/output src/contracts/abis/*.json",
    "types": "tsc -p tsconfig.json --noEmit",
    "postinstall": "npx puppeteer browsers install"
  },
  "lint-staged": {
    "**/!(next-env).{js,jsx,ts,tsx}": [
      "eslint --max-warnings=0",
      "prettier -w"
    ],
    "**/*.{json,css,scss,md,webmanifest}": [
      "prettier -w"
    ]
  },
  "dependencies": {
    "@coinbase/wallet-sdk": "^3.7.2",
    "@compilot/js-sdk": "^2.111.0",
    "@compilot/react-sdk": "^2.114.0",
    "@compilot/web-sdk-wallet-wagmi": "^2.49.0",
    "@cowprotocol/widget-lib": "^0.20.0",
    "@cowprotocol/widget-react": "^0.14.0",
    "@emotion/cache": "^11.11.0",
    "@emotion/react": "^11.11.4",
    "@emotion/server": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@kasufinance/kasu-sdk": "^2.1.0",
    "@mui/icons-material": "^6.1.6",
    "@mui/material": "^6.1.6",
    "@mui/material-nextjs": "^6.1.6",
    "@nivo/bar": "^0.87.0",
    "@nivo/grid": "^0.87.0",
    "@nivo/line": "^0.87.0",
    "@nivo/pie": "^0.87.0",
    "@nivo/tooltip": "^0.87.0",
    "@nivo/voronoi": "^0.87.0",
    "@privy-io/react-auth": "3.8.0",
    "@privy-io/wagmi": "^2.0.3",
    "@solana-program/memo": "^0.10.0",
    "@solana-program/system": "^0.10.0",
    "@solana-program/token": "^0.9.0",
    "@solana/kit": "^5.0.0",
    "@sparticuz/chromium-min": "^133.0.0",
    "@tanstack/react-query": "^5.74.11",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "d3-shape": "^3.2.0",
    "dayjs": "^1.11.10",
    "ethers": "^5.7.2",
    "graphql-request": "^7.4.0",
    "html-to-image": "^1.11.13",
    "next": "^15.4.8",
    "pino-pretty": "^11.0.0",
    "pluno-web-chatbot": "^1.0.8",
    "puppeteer": "^24.4.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-markdown": "^7.0.0",
    "sass": "^1.69.5",
    "sharp": "^0.33.5",
    "styled-components": "^6.1.2",
    "swr": "^2.3.4",
    "typescript": "^5.6.3",
    "viem": "^2.43.1",
    "wagmi": "^2.15.5",
    "xhr2": "^0.2.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@next/eslint-plugin-next": "15.0.2",
    "@typechain/ethers-v5": "^11.1.2",
    "@types/d3-shape": "^3.1.6",
    "@types/node": "^20",
    "@types/styled-components": "^5.1.34",
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "@typescript-eslint/parser": "^6.16.0",
    "eslint": "^8.56.0",
    "eslint-config-next": "15.0.2",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "graphql": "^16.8.2",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "prettier": "^3.1.1",
    "typechain": "^8.3.2"
  }
}
